# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/autotest_dev

# JWT Secret
JWT_SECRET=your-super-secret-jwt-key-here

# Environment Configuration
NODE_ENV=development

# Test Environment URLs (for self-testing)
TEST_BASE_URL=https://test.yourdomain.com
TEST_API_URL=https://test.yourdomain.com/api
TEST_TARGET_URL=https://test.yourdomain.com

# Staging Environment URLs
STAGING_BASE_URL=https://staging.yourdomain.com
STAGING_API_URL=https://staging.yourdomain.com/api
STAGING_TARGET_URL=https://staging.yourdomain.com

# Production Environment URLs
PROD_BASE_URL=https://yourdomain.com
PROD_API_URL=https://yourdomain.com/api
PROD_TARGET_URL=https://yourdomain.com

# Test User Credentials for Test Environment
TEST_ADMIN_EMAIL=admin@test.yourdomain.com
TEST_ADMIN_PASSWORD=admin123
TEST_USER_EMAIL=user@test.yourdomain.com
TEST_USER_PASSWORD=user123

# Test User Credentials for Staging Environment
STAGING_ADMIN_EMAIL=admin@staging.yourdomain.com
STAGING_ADMIN_PASSWORD=admin123
STAGING_USER_EMAIL=user@staging.yourdomain.com
STAGING_USER_PASSWORD=user123

# Test User Credentials for Production Environment
PROD_ADMIN_EMAIL=admin@yourdomain.com
PROD_ADMIN_PASSWORD=admin123
PROD_USER_EMAIL=user@yourdomain.com
PROD_USER_PASSWORD=user123

# Self-Testing Configuration
SELF_TEST_ENABLED=true
SELF_TEST_INTERVAL=3600000  # 1 hour in milliseconds
SELF_TEST_TEAM_ID=1

# GitHub Integration (for test data)
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
