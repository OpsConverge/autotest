{
  "name": "test-management-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --report-unused-disable-directives",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/test/components/ src/test/hooks/ src/test/utils/ src/test/unit-jest/ src/test/unit-ava/ src/test/unit-tap/",
    "test:integration": "vitest run src/test/integration/ src/test/integration-jest/ src/test/integration-ava/ src/test/integration-tap/ src/test/integration-supertest/",
    "test:api": "vitest run src/test/api/ src/test/api-supertest/ src/test/api-karate/ src/test/api-restassured/",
    "test:e2e": "vitest run src/test/e2e/ src/test/e2e-selenium/",
    "test:pages": "vitest run src/test/pages/",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:pages",
    "test:ci": "vitest run --coverage --reporter=verbose src/test/components/ src/test/hooks/ src/test/utils/ src/test/integration/ src/test/api/ src/test/pages/",
    "test:unit:jest": "jest src/test/unit-jest/",
    "test:integration:jest": "jest src/test/integration-jest/",
    "test:coverage:jest": "jest --coverage src/test/unit-jest/",
    "test:unit:ava": "ava src/test/unit-ava/",
    "test:integration:ava": "ava src/test/integration-ava/",
    "test:coverage:ava": "ava --coverage src/test/unit-ava/",
    "test:unit:tap": "tap src/test/unit-tap/",
    "test:integration:tap": "tap src/test/integration-tap/",
    "test:coverage:tap": "tap --coverage src/test/unit-tap/",
    "test:integration:supertest": "vitest run src/test/integration-supertest/",
    "test:api:supertest": "vitest run src/test/api-supertest/",
    "test:api:karate": "vitest run src/test/api-karate/",
    "test:api:restassured": "vitest run src/test/api-restassured/",
    "test:e2e:playwright": "playwright test",
    "test:e2e:cypress": "cypress run",
    "test:e2e:selenium": "vitest run src/test/e2e-selenium/",
    "test:performance:k6": "k6 run tests/performance/k6-load-test.js",
    "test:performance:jmeter": "jmeter -n -t tests/performance/jmeter-load-test.jmx -l reports/jmeter-results.jtl",
    "test:performance:gatling": "gatling -s LoadTest -rf reports/gatling",
    "test:performance:all": "npm run test:performance:k6 && npm run test:performance:jmeter && npm run test:performance:gatling",
    "test:watch": "vitest --watch",
    "test:backend": "cd backend && npm test",
    "test:backend:unit": "cd backend && npm run test:unit",
    "test:backend:integration": "cd backend && npm run test:integration",
    "test:backend:api": "cd backend && npm run test:api",
    "test:backend:all": "cd backend && npm run test:all",
    "test:pipeline:unit": "npm run test:unit:jest && npm run test:unit:ava && npm run test:unit:tap",
    "test:pipeline:integration": "npm run test:integration:jest && npm run test:integration:ava && npm run test:integration:tap && npm run test:integration:supertest",
    "test:pipeline:api": "npm run test:api:supertest && npm run test:api:karate && npm run test:api:restassured",
    "test:pipeline:e2e": "npm run test:e2e:playwright && npm run test:e2e:cypress && npm run test:e2e:selenium",
    "test:pipeline:performance": "npm run test:performance:all",
    "test:pipeline:all": "npm run test:pipeline:unit && npm run test:pipeline:integration && npm run test:pipeline:api && npm run test:pipeline:e2e && npm run test:pipeline:performance"
  },
  "dependencies": {
    "@base44/sdk": "^0.3.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.294.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "sonner": "^1.2.4",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/register": "^7.22.0",
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^1.0.0",
    "ava": "^6.0.0",
    "cypress": "^13.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.0.1",
    "k6": "^0.47.0",
    "node-fetch": "^3.3.2",
    "selenium-webdriver": "^4.15.0",
    "supertest": "^6.3.3",
    "tap": "^18.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vitest-dom": "^0.1.0"
  }
}
